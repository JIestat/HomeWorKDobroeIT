
public  class Pitomnik {

    private  Animal [] animals = new Animal[8];

    public void addAnimal(Animal[] array){
        for (int i = 0; i < animals.length ; i++) {
            if (array.length == animals.length) {
                animals[i]=array[i] ;
            }else {
                // тут  будет ошибка java.lang.ArrayIndexOutOfBoundsException
                System.out.println("Питомник переполнен");
            }
        }
    }


    public void printAnimalToPitomnik(Animal[] animals , int start, int end){
        for (int i = 0; i < animals.length ; i++) {
            int age = animals[i].getAge();
            if (animals[i].getClass().equals(Cat.class)){
                if (age >= start && age <= end){
                    System.out.println(animals[i]);
                }
            }else {
                if (age >= start && age <= end){
                    System.out.println(animals[i]);
                }
            }
        }
    }





    public Animal[] getAnimals() {
        return animals;
    }

    public void setAnimals(Animal[] animals) {
        this.animals = animals;
    }
}
